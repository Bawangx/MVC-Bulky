@page
@model LockoutModel
@{
    // Set judul halaman
    ViewData["Title"] = "Locked out";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1> <!-- Judul halaman dengan warna merah untuk menandakan error/peringatan -->
    <p class="text-danger">
        This account has been locked out, please try again later.
        <!-- Pesan informasi bahwa akun terkunci dan harus menunggu -->
    </p>
</header>